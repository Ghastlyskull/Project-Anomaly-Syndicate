    <!-- Long Syndicate Door -->
    <ThingDef ParentName="NonDeconstructibleAncientBuildingBase">
      <defName>Gha_LongAncientDoor</defName>
      <label>ancient facility door</label>
      <description>A large facility door, typically found in most syndicate facilities.</description>
      <rotatable>true</rotatable>
      <graphicData>
        <texPath>Things/Buildings/Ruins/AncientVaultDoor_Long</texPath>
        <graphicClass>Graphic_Multi</graphicClass>
        <drawSize>(4,1)</drawSize>
        <shadowData>
          <volume>(1.1, 0.66, 2.4)</volume>
        </shadowData>
      </graphicData>
      <size>(4,1)</size>
      <fillPercent>0.5</fillPercent>
      <castEdgeShadows>true</castEdgeShadows>
      <passability>PassThroughOnly</passability>
      <pathCost>50</pathCost>
      <statBases>
        <MaxHitPoints>2000</MaxHitPoints>
      </statBases>
      <leaveResourcesWhenKilled>false</leaveResourcesWhenKilled>
      <!--
      <killedLeavings>
        <ChunkSlagSteel>6</ChunkSlagSteel>
      </killedLeavings>
      -->
    </ThingDef>

